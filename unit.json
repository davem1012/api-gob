{
    "listeners": {
        "*:8001": {
            "pass": "routes"
        }
    },
    "routes": [
        {
            "match": {
                "uri": ["!/index.php", "!*.css", "!*.js", "!*.png", "!*.jpg"]
            },
            "action": {
                "pass": "applications/laravel"
            }
        },
        {
            "action": {
                "share": "/var/www/html/public$uri",
                "fallback": {
                    "pass": "applications/laravel"
                }
            }
        }
    ],
    "applications": {
        "laravel": {
            "type": "php",
            "root": "/var/www/html/public",
            "script": "index.php",
            "processes": 4,
            "options": {
                "admin": {
                    "memory_limit": "512M",
                    "upload_max_filesize": "256M",
                    "post_max_size": "256M"
                }
            }
        }
    }
}
